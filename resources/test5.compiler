{//annotation test
    int i;int j;float v;float x;float[100] a;
    i=1;
    j=2;
    v=1.0;
    x=2.0;
    a=[3.0,4.0];
    while(true){
        do{
            i=i+1;//this is a anotation in while
        }while(a[i]<v);
        do{
            j=j+1;
        }while(a[j]>v);
        if(i>j) break;
        x=a[i];a[i]=a[j];a[j]=x;
    }
};